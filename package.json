{
  "name": "crud",
  "version": "1.0.0",
  "description": "TypeScript CRUD",
  "main": "./build/server.js",
  "engines": {
    "node": "18",
    "nvm": "8"
  },
  "scripts": {
    "prepare": "husky install",
    "pm2:start": "pm2 start npm --name=\"CRUD\" -- start",
    "pm2:restart": "pm2 restart \"CRUD\"",
    "pm2:delete": "pm2 delete \"CRUD\"",
    "start": "npm run build:clean && cross-env node --no-warnings ./build/server.js",
    "start:ts": "node  --trace-deprecation -r ts-node/register -r tsconfig-paths/register ./src/server.ts",
    "dev": "nodemon --ext \"ts,js,json\" --ignore \"node_modules\" --exec npm -- run start:ts",
    "build": "npx tsc",
    "build:clean": "rimraf build && npx tsc",
    "lint": "eslint --fix . --ext .ts",
    "copy-env": "cp .env.sample .env",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "author": "Azim Ansari",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.213.0",
    "@types/helmet": "^4.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/speakeasy": "^2.0.7",
    "aws-sdk": "^2.1258.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "body-parser": "*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "joi": "^17.8.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongodb": "*",
    "mongoose": "^6.7.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-device-detector": "^2.0.12",
    "qrcode": "^1.5.1",
    "recachegoose": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "speakeasy": "^2.0.0",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.189",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@modules": "build/modules",
    "@models": "build/models",
    "@config": "build/core/config",
    "@core": "build/core",
    "@middlewares": "build/middlewares",
    "@helpers": "build/helpers",
    "@jobs": "build/jobs"
  }
}